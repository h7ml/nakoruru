    "antd": "^5.6.3",
    "axios": "^1.4.0",
    "babylonjs": "^6.10.0",
    "cannon": "^0.6.2",
    "classnames": "^2.3.2",
    "http-proxy-middleware": "^2.0.6",
    "less": "^4.1.3",
    "lodash-es": "^4.17.21",
    "mobx": "^6.9.0",
    "monaco-editor": "^0.39.0",
    "prop-types": "^15.8.1",
    "qs": "^6.11.2",
    "rc-field-form": "1.33.0",
    "react": "^18.2.0",
